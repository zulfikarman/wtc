<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zulfiabe.wtcshop"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoffeTimeActivity"
            android:label="@string/title_activity_coffe_time" >
        </activity>
        <activity
            android:name=".DapurIgaActivity"
            android:label="@string/title_activity_dapur_iga" >
        </activity>
        <activity
            android:name=".ShidaActivity"
            android:label="@string/title_activity_shida" >
        </activity>
    </application>

</manifest>
